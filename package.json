{
  "name": "yc-ui-pro",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "description": "A Vue 3 component library.",
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "components",
    "yc-ui"
  ],
  "author": "2429162154@qq.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/boy-twilight-lee/yc-ui"
  },
  "scripts": {
    "dev": "vite --host ",
    "build": "vite build",
    "preview": "vite preview",
    "release": "vite build && npm publish",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "npx prettier --write --loglevel warn 'src/**/*.{js,ts,json,tsx,css,less,vue,html,md}'"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.js"
    },
    "./es": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js"
    },
    "./lib": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js"
    },
    "./es/*": {
      "types": "./es/*.d.ts",
      "import": "./es/*"
    },
    "./lib/*": {
      "types": "./lib/*.d.ts",
      "require": "./lib/*"
    }
  },
  "dependencies": {
    "@vueuse/core": "^12.0.0",
    "b-tween": "^0.3.3",
    "dayjs": "^1.11.11",
    "nanoid": "^5.1.2",
    "tinycolor2": "^1.6.0",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.55.1",
    "@types/node": "^20.12.8",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.31.0",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.8.5"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "lint-staged": {
    "*.{vue,ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
